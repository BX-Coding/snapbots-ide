{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/modal/generation",
      "destination": "https://eucalyptus--simulation-endpoint.modal.run/generation"
    },
    {
      "source": "/api/modal/retrieve-code",
      "destination": "https://eucalyptus--simulation-endpoint.modal.run/retrieve-code"
    },
    {
      "source": "/api/modal/retrieve-names",
      "destination": "https://eucalyptus--simulation-endpoint.modal.run/retrieve-names"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  }
} 